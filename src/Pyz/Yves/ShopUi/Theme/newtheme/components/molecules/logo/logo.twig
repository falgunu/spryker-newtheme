{% extends model('component') %}
{% import _self as component %}

{% define config = {
    name: 'logo',
    tag: 'div'
} %}

{% define data = {
    url: '/'
} %}

{% define attributes = {
    src: null,
    alt: 'Logo'
} %}

{% block attributes %}{% endblock %}

{% block body %}
    <a href="{{data.url}}" class="{{ component.renderClass(config.name ~ '__link', modifiers) }}">
       <img src="/assets/static/images/logo.png"/>
    </a>
{% endblock %}
