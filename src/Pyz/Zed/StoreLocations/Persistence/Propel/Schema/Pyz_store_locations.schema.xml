<?xml version="1.0"?>
<database xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="zed"
xsi:noNamespaceSchemaLocation="http://static.spryker.com/schema-01.xsd"
namespace="Orm\Zed\StoreLocations\Persistence"
package="src.Orm.Zed.StoreLocations.Persistence">
<table name="pyz_store_locations" idMethod="native" allowPkInsert="true"
phpName="PyzStoreLocations">
