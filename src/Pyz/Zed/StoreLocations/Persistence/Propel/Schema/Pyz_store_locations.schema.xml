primaryKey="true" autoIncrement="true"/>
<column name="storename" required="true" type="VARCHAR" size="255"/>
<column name="street" required="true" type="VARCHAR" size="255"/>
<column name="city" required="true" type="VARCHAR" size="255"/>
<column name="region" required="true" type="VARCHAR" size="255"/>
<column name="country" required="true" type="VARCHAR" size="255"/>
<column name="zipcode" required="true" type="VARCHAR" size="255"/>
<column name="longitude" required="true" type="VARCHAR" size="255"/>
<column name="latitude" required="true" type="VARCHAR" size="255"/>
<column name="storeimage" required="true" type="clob" />
<id-method-parameter value="pyz_storelocations_pk_seq"/>
</table>
</database>
